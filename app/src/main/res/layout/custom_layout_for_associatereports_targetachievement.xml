<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="160dp"
    android:background="#ffffff"
    android:padding="10dp">

<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    android:padding="10dp"
    android:weightSum="10">
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_alignParentLeft="true"
        android:layout_weight="6"
        android:gravity="left">
        <TextView
            android:id="@+id/customlayoutassociatereports_TargetProductNameTextViewID"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=""
            android:textStyle="normal|bold"
            android:textSize="18sp"
            android:textColor="#464646"
            android:gravity="left"
            android:layout_gravity="left"
            android:layout_alignParentLeft="true"
            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="0dp"
        android:layout_alignParentRight="true"
        android:layout_weight="4"
        android:gravity="right">
        <TextView
            android:id="@+id/customlayoutassociatereports_TargetPercentageTextViewID"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=""
            android:textStyle="normal|bold"
            android:textSize="18sp"
            android:textColor="#464646"
            android:gravity="right"
            android:layout_gravity="right"
            android:paddingRight="0dp"
            android:paddingLeft="0dp"
            android:layout_marginLeft="0dp"
            android:layout_alignParentRight="true"
            />
        </LinearLayout>
</LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingTop="15dp"
        android:weightSum="4"
        android:layout_gravity="left|center">

        <LinearLayout
            android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical"
            android:layout_gravity="center"
            android:gravity="left">
            <TextView
                android:id="@+id/customlayoutassociatereports_TargetYTDTAGTextViewID"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Target \nYTD"
                android:textStyle="normal"
                android:textSize="14sp"
                android:layout_marginBottom="0dp"
                android:textColor="#464646"
                android:gravity="center"
                android:layout_gravity="center"
                />
            <TextView
                android:id="@+id/customlayoutassociatereports_TargetYTDValueTextViewID"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textStyle="normal"
                android:textSize="14sp"
                android:layout_marginTop="10dp"
                android:textColor="#464646"
                android:gravity="center"
                android:layout_gravity="center"
                android:paddingLeft="0dp"
                />
        </LinearLayout>

        <View
            android:id="@+id/customlayoutcustomViewAttendanceApprovalID9"
            android:layout_width="1dp"
            android:layout_height="100dp"
            android:layout_marginBottom="0dp"
            android:background="#d3d5d7"
            android:alpha="0.8"
            />

        <LinearLayout
            android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical"
            android:layout_gravity="center"
            android:gravity="left">
            <TextView
                android:id="@+id/customlayoutassociatereports_AchieveYTDTAGTextViewID"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Achieved \nYTD"
                android:textStyle="normal"
                android:textSize="14sp"
                android:layout_marginBottom="0dp"
                android:textColor="#464646"
                android:gravity="center"
                android:layout_gravity="center"
                />
            <TextView
                android:id="@+id/customlayoutassociatereports_AchieveYTDValueTextViewID"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textStyle="normal"
                android:textSize="14sp"
                android:layout_marginTop="10dp"
                android:textColor="#464646"
                android:gravity="center"
                android:layout_gravity="center"
                android:paddingLeft="0dp"
                />
        </LinearLayout>

        <View
            android:id="@+id/customlayoutcustomViewAttendanceApprovalID99"
            android:layout_width="1dp"
            android:layout_height="100dp"
            android:layout_marginBottom="0dp"
            android:background="#d3d5d7"
            android:alpha="0.8"
            />

        <LinearLayout
            android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical"
            android:layout_gravity="center"
            android:gravity="left">
            <TextView
                android:id="@+id/customlayoutassociatereports_TargetMTDTAGTextViewID"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Target \nMTD"
                android:textStyle="normal"
                android:textSize="14sp"
                android:layout_marginBottom="0dp"
                android:textColor="#464646"
                android:gravity="center"
                android:layout_gravity="center"
                />
            <TextView
                android:id="@+id/customlayoutassociatereports_TargetMTDValueTextViewID"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textStyle="normal"
                android:textSize="14sp"
                android:layout_marginTop="10dp"
                android:textColor="#464646"
                android:gravity="center"
                android:layout_gravity="center"
                android:paddingLeft="0dp"
                />
        </LinearLayout>

        <View
            android:id="@+id/customlayoutcustomViewAttendanceApprovalID999"
            android:layout_width="1dp"
            android:layout_height="100dp"
            android:layout_marginBottom="0dp"
            android:background="#d3d5d7"
            android:alpha="0.8"
            />

        <LinearLayout
            android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical"
            android:layout_gravity="center"
            android:gravity="left">
            <TextView
                android:id="@+id/customlayoutassociatereports_AchieveMTDTAGTextViewID"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Achieved \nMTD"
                android:textStyle="normal"
                android:textSize="14sp"
                android:layout_marginBottom="0dp"
                android:textColor="#464646"
                android:gravity="center"
                android:layout_gravity="center"
                />
            <TextView
                android:id="@+id/customlayoutassociatereports_AchieveMTDValueTextViewID"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textStyle="normal"
                android:textSize="14sp"
                android:layout_marginTop="10dp"
                android:textColor="#464646"
                android:gravity="center"
                android:layout_gravity="center"
                android:paddingLeft="0dp"
                />
        </LinearLayout>



    </LinearLayout>


    <!--<View
        android:id="@+id/customlayoutcustomViewAttendanceApprovalID19"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginBottom="0dp"
        android:background="#d3d5d7"
        android:alpha="0.8"
        />-->



</LinearLayout>
<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="0dp"
    android:background="#ffffff"
    tools:context="smartracgrohe.KYCActivity">

    <RelativeLayout
        android:id="@+id/KYCPageUpperLinearLayoutID1"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:orientation="horizontal"
        android:layout_alignParentTop="true"
        android:background="@color/colorPrimary">
        <ImageView
            android:id="@+id/KYCtopbarBackiconImageViewID"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/leftarrowicon"
            android:layout_gravity="center"
            android:layout_marginLeft="5dp"
            android:layout_marginBottom="0dp"
            android:layout_alignParentLeft="true"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"/>
        <TextView
            android:layout_centerInParent="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:gravity="center"
            android:text="KYC"
            android:textStyle="bold"
            android:textColor="#ffffff"
            android:id="@+id/mytext"
            android:textSize="16sp"
            />
    </RelativeLayout>

<ScrollView
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_centerInParent="true"
    android:layout_below="@+id/KYCPageUpperLinearLayoutID1"
    android:layout_above="@+id/botombarKYCPageSubmitLayoutID">
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:padding="5dp">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Aadhar Details"
        android:textStyle="normal"
        android:textSize="14sp"
        android:textColor="#111111"
        android:gravity="left"
        android:paddingLeft="5dp"
        android:paddingRight="5dp"
        android:paddingTop="10dp"
        android:layout_gravity="left"/>
    <Spinner
        android:id="@+id/KYCPageAadharNoSpinnerID"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:prompt="@string/spinner_title9"
        android:entries="@array/aadharno_spinnerItems"
        android:layout_margin="2dp"
        android:textSize="14sp"
        android:textColor="#111111"
        android:drawSelectorOnTop="true"
        android:background="@drawable/backdropdownfieldblack"
        />
    <LinearLayout
        android:id="@+id/KYCPageAadharNoLinearLayoutID"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:padding="2dp"
        android:weightSum="10"
        android:visibility="gone">
        <TextView
            android:id="@+id/KYCPageAadharNoTextViewID"
            android:layout_width="250dp"
            android:layout_height="wrap_content"
            android:text="Aadhar No."
            android:textStyle="normal"
            android:textSize="14sp"
            android:textColor="#111111"
            android:gravity="left"
            android:paddingLeft="5dp"
            android:paddingRight="5dp"
            android:paddingTop="10dp"
            android:layout_gravity="left"
            android:layout_weight="6"/>
        <EditText
            android:id="@+id/KYCPageAadharNoValueEditTextID"
            android:layout_width="150dp"
            android:layout_height="35dp"
            android:paddingLeft="10dp"
            android:inputType="text"
            android:digits="ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789"
            android:lines="1"
            android:layout_marginTop="2dp"
            android:layout_marginLeft="2dp"
            android:layout_marginRight="2dp"
            android:maxLength="60"
            android:textSize="14sp"
            android:gravity="start|center_vertical"
            android:hint="Aadhar No."
            android:theme="@style/MyEditTextTheme"
            android:background="@drawable/edittextfieldback"
            android:textColor="#505050"
            android:textStyle="normal"
            android:focusableInTouchMode="true"
            android:layout_weight="4"
            android:visibility="gone"
            />
